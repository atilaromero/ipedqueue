{
  "mongodb":{
    "db": "development"
  },
  "iped":{
    "javabin": "/usr/bin/docker",
    "javaoptions": [
      "run",
      "--rm",
      "--volumes-from=ipedqueue_iped-queue_1",
      "--volumes-from=iped-volumes",
      "setecrs/runiped",
      "java",
      "-Djava.awt.headless=true",
      "-Xmx100G",
      "-Xms1G",
      "-jar",
      "/root/iped/iped.jar",
      "--nogui",
      "--nologfile",
      "--portable"
    ]
  },
  "listenport": 8880,
  "precmd": "rm -r \"$(pwd)\"/development/SARD/ || true",
  "polling_interval": 2000
}
