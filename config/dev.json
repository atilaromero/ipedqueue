{
  "redis":{
    "host": "localhost",
    "port": "6379"
  },
  "mongodb":{
    "host": "localhost",
    "port": "27017",
    "db": "development"
  },
  "iped":{
    "javabin": "/usr/bin/docker",
    "javaoptions": [
      "run",
      "--rm",
      "-v", "/storages/local/:/storages/local/",
      "--volumes-from=iped-volumes",
      "runiped",
      "java",
      "-Xmx100G",
      "-Xms1G",
      "-jar",
      "/root/iped/iped.jar",
      "--nogui",
      "--nologfile",
      "--portable"
    ]
  },
  "listenport": 8880,
  "precmd": "",
  "polling_interval": 2000
}
