{
  "mongodb":{
    "host": "localhost",
    "port": "27017",
    "db": "sard"
  },
  "iped":{
    "javabin": "/usr/bin/docker",
    "javaoptions": [
      "run",
      "--rm",
      "--volumes-from=iped-volumes",
      "deepcase/iped:debian-3.12.4-NIOFS",
      "java",
      "-Djava.awt.headless=true",
      "-d64",
      "-Xmx100G",
      "-Xms1G",
      "-jar",
      "/root/iped/iped.jar",
      "--nogui",
      "--nologfile",
      "--portable"
    ]
  },
  "listenport": 80,
  "precmd": "",
  "polling_interval": 2000,
  "EVENTS_URL": "http://iped-queue-events:80/"
}
