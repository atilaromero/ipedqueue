{
  "mongodb":{
    "host": "localhost",
    "port": "27017",
    "db": "sard"
  },
  "iped":{
    "javabin": "/usr/bin/docker",
    "javaoptions": [
      "run",
      "--rm",
      "--volumes-from=iped-volumes",
      "deepcase/iped:3.12.4",
      "java",
      "-Djava.awt.headless=true",
      "-Xmx32G",
      "-Xms1G",
      "-jar",
      "/root/iped/iped.jar",
      "--nogui",
      "--nologfile",
      "--portable"
    ]
  },
  "listenport": 80,
  "precmd": "",
  "polling_interval": 2000
}
